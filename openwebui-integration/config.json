{
  "adEngine": {
    "host": "localhost",
    "port": 8080,
    "endpoint": "/api/process-message",
    "timeout": 5000
  },
  "openwebui": {
    "host": "localhost",
    "port": 3000,
    "wsEndpoint": "/ws",
    "apiEndpoint": "/api/v1"
  },
  "integration": {
    "port": 3001,
    "host": "localhost",
    "enableWebSocket": true,
    "enableREST": true
  },
  "adSettings": {
    "enabled": true,
    "minRelevanceScore": 0.3,
    "maxAdsPerResponse": 1,
    "responseTemplate": "ðŸ’¡ {adResponse}",
    "enableAnalytics": true,
    "cacheEnabled": true,
    "cacheTTL": 30000
  },
  "userPreferences": {
    "defaultInterests": ["technology", "fashion", "travel"],
    "defaultBlockedCategories": ["gambling", "alcohol"],
    "adPreferencesEnabled": true
  },
  "logging": {
    "level": "info",
    "file": "logs/integration.log",
    "maxSize": "10m",
    "maxFiles": 5
  }
}
